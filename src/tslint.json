{
  "rulesDirectory": [
    "../node_modules/tslint-eslint-rules/dist/rules",
    "../node_modules/codelyzer"
  ],
  "rules": {
    "curly": true,
    "indent": [true, 2, "spaces"],
    "quotemark": [true, "single"],
    "semicolon": true,
    "class-name": true,
    "eofline": true,
    "no-arg": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-unused-expression": true,
    "no-var-keyword": true,
    "one-line": [true, "check-catch", "check-finally", "check-else"],
    "no-conditional-assignment": true,
    "no-empty": true,
    "no-switch-case-fall-through": true,
    "forin": true,
    "no-bitwise": true,
    "no-consecutive-blank-lines": [true],
    "no-eval": true,
    "no-trailing-whitespace": true,
    "no-angle-bracket-type-assertion": true,
    "triple-equals": [true, "allow-null-check"],
    "use-isnan": true,
    "one-variable-per-declaration": [true, "ignore-for-loop"],
    "no-unsafe-finally": true,
    "object-literal-key-quotes": [true, "as-needed"],
    "object-literal-shorthand": true,
    "no-constant-condition": true,
    "no-duplicate-case": true,
    "no-empty-character-class": true,
    "no-ex-assign": true,
    "no-extra-boolean-cast": true,
    "no-extra-semi": true,
    "no-sparse-arrays": true,
    "block-spacing": [
      true,
      "always"
    ],
    "brace-style": [
      true,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "array-type": [true, "array"],
    "prefer-for-of": true,
    "ter-prefer-arrow-callback": [true],
    "prefer-const": true,
    "space-before-function-paren": [true, "never"],
    "import-spacing": true,
    "arrow-return-shorthand": [true],
    "no-misused-new": true,
    "prefer-method-signature": true,
    "no-duplicate-super": true,
    "prefer-template": [true, "allow-single-concat"],
    "no-unnecessary-callback-wrapper": true,
    "no-invalid-template-strings": true,
    "no-object-literal-type-assertion": true,
    "prefer-switch": true,
    "number-literal-format": true,
    "interface-name": [true, "always-prefix"],
    "ban-types": [true, ["Object", "Use object instead."]],
    "no-empty-interface": true,
    "member-access": true,
    "typedef": [true, "call-signature", "parameter"],
    "trailing-comma": [true,
      {
        "multiline": {
          "arrays": "always",
          "functions": "always",
          "objects": "always"
        },
        "singleline": "never"
      }
    ],
    "no-this-assignment": true,
    "encoding": true,
    "prefer-object-spread": true,
    "binary-expression-operand-order": true,
    "no-duplicate-imports": true,
    "banana-in-box": true,
    "no-attribute-parameter-decorator": true,
    "no-unused-css": true,
    "use-life-cycle-interface": true,
    "use-pipe-decorator": true,
    "use-pipe-transform-interface": true,
    "component-class-suffix": true,
    "component-selector": ["element", "", "kebab-case"],
    "directive-class-suffix": true,
    "directive-selector": ["attribute", "", "camelCase"],
    "use-host-property-decorator": true,
    "use-input-property-decorator": true,
    "use-output-property-decorator": true,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ],
    "no-return-await": true,
    "no-duplicate-switch-case": true,
    "no-unnecessary-class": ["no-empty-class"],
    "ban-comma-operator": true,
    "no-inferrable-types": [true, "ignore-params", "ignore-properties"]
  }
}
